# Supabase Configuration
SUPABASE_URL=https://your-project.supabase.co

# Use either anon key OR service key
# Service key has full access and bypasses RLS policies (recommended for backups)
SUPABASE_ANON_KEY=your-anon-key-here
SUPABASE_SERVICE_KEY=your-service-role-key-here  # Optional but recommended

SUPABASE_BUCKET_NAME=audio
SUPABASE_AUDIO_PATH=

# AWS S3 Configuration
S3_BUCKET_NAME=your-s3-bucket-name
S3_ACCESS_KEY_ID=your-aws-access-key-id
S3_SECRET_ACCESS_KEY=your-aws-secret-access-key
S3_REGION=us-east-1
S3_PREFIX=supabase-backups/audio

# Backup Configuration
BACKUP_CRON=0 2 * * *
TEMP_DIR=/tmp/supabase-audio-backups
FILE_EXTENSIONS=.mp3,.wav,.m4a,.aac,.ogg,.flac
BATCH_SIZE=5